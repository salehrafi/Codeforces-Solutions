#include<bits/stdc++.h>
using namespace std;
int main() {
	int r,c,color = 0;
	cin >> r >> c;
	char arr[r][c];

	for(int i=0;i<r;i++){
		for(int j=0;j<c;j++){
			cin >> arr[i][j];
			if(arr[i][j] == 'C' || arr[i][j] == 'M' ||arr[i][j] == 'Y'){
				color = 1;
			}
		}
	}
	if(color != 0){
		cout << "#Color" << endl;
	}else{
		cout << "#Black&White" << endl;
	}
	return 0;
}
